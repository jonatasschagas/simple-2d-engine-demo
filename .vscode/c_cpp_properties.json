{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/simple-2d-engine/engine/**",
                "${workspaceFolder}/simple-2d-engine/service-implementation/graphics/opengl/**",
                "${workspaceFolder}/simple-2d-engine/third-party/**",
                "${workspaceFolder}/simple-2d-engine/third-party/glad/includes/**",
                "${workspaceFolder}/src/**"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Library/Developer/CommandLineTools/SDKs/MacOSX10.15.sdk/System/Library/Frameworks"
            ],
